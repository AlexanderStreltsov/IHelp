{
  "name": "ihelp",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://alexanderstreltsov.github.io/IHelp/",
  "scripts": {
    "start": "set PUBLIC_URL= && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color --fix",
    "lint:fix": "npm run lint --fix",
    "prettier": "prettier --check --loglevel debug \"src/**/*.{ts,tsx,scss,css,json}\"",
    "prettier:format": "prettier --write \"src/**/*.{ts,tsx,scss,css,json}\"",
    "build:storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "isready": "npm run prettier:format && npm run lint && npm run build && npm run build:storybook"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@pbe/react-yandex-maps": "^1.2.4",
    "date-fns": "^2.30.0",
    "nanoid": "^4.0.2",
    "react": "^18.2.0",
    "react-datepicker": "^4.12.0",
    "react-dom": "^18.2.0",
    "react-moment": "^1.1.3",
    "react-router-dom": "^6.11.2",
    "sass": "^1.62.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-interactions": "^7.0.12",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/blocks": "^7.0.12",
    "@storybook/preset-create-react-app": "^7.0.12",
    "@storybook/react": "^7.0.12",
    "@storybook/react-webpack5": "^7.0.12",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.31",
    "@types/react": "^18.2.6",
    "@types/react-datepicker": "^4.11.2",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "prettier": "^2.8.8",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "prop-types": "^15.8.1",
    "react-scripts": "5.0.1",
    "storybook": "^7.0.12",
    "storybook-addon-react-router-v6": "^1.0.2",
    "typescript": "^4.9.5",
    "webpack": "^5.83.1"
  }
